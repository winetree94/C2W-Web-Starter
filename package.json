{
  "name": "@c2w/base",
  "packageManager": "yarn@4.3.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "app:dev": "yarn workspace @c2w/browser_runtime dev",
    "app:build": "yarn workspace @c2w/browser_runtime build",
    "binary:install": "yarn workspace @c2w/builder binary:install",
    "wasm:build": "yarn workspace @c2w/builder wasm:build && yarn workspace @c2w/container wasm:copy"
  }
}
